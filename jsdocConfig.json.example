{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./jsDocs/gen",
    "access": "all",
    "recurse": true,
    "sort": false
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize",
    "jsdoc-plantuml"
  ],
  "plantuml": {
    "puml": {
      "create": true,
      "destination": "jsDocs/plantuml"
    },
    "images": {
      "create": true,
      "destination": "jsDocs/gen/images",
      "defaultFormat": "png",
      "replaceWithImage": true
    }
  }
}
